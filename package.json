{
  "name": "@nickchristensen/ppls",
  "description": "A node-based paperless-ngx cli",
  "version": "1.2.0",
  "author": "Nick Christensen",
  "bin": {
    "ppls": "bin/run.js"
  },
  "bugs": "https://github.com/nickchristensen/ppls/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "date-fns": "^3.6.0",
    "tty-table": "^5.0.0",
    "yocto-spinner": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "husky": "^9.1.7",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/nickchristensen/ppls",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "ppls",
    "dirname": "ppls",
    "commands": "./dist/commands",
    "helpOptions": {
      "hideAliasesFromRoot": true
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage ppls configuration settings"
      },
      "correspondents": {},
      "custom-fields": {},
      "documents": {},
      "document-types": {},
      "tags": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickchristensen/ppls.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepare": "husky install",
    "prepack": "npm run build",
    "postbuild": "oclif manifest && oclif readme --no-aliases",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme --no-aliases && git add README.md"
  },
  "types": "dist/index.d.ts"
}
